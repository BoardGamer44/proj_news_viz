    "import re, urllib.parse\n",
    "\n",
    "def get_section(u):\n",
    "    path = urllib.parse.urlsplit(u).path.strip('/')\n",
    "    final = []\n",
    "    for p in path.split('/')[:-1]:\n",
    "        if not p.isdigit() and not re.match('\\d?\\d\\w\\w\\w20\\d\\d', p):\n",
    "            final.append(p)\n",
    "    return '/'.join(final)\n",
    "print('section=', get_section('http://www.rus.newsru.ua/finance/01sep2014/1se...'))"
