    "def get_edition(src, u):\n",
    "    s = urllib.parse.urlsplit(u).netloc\n",
    "    while s.startswith('www.'):\n",
    "        s = s[4:]\n",
    "    if s == src: return '-'\n",
    "    return s.rsplit('.'+src, 1)[0]\n",
