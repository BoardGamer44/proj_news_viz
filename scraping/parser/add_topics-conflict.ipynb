    "\n",
    "def load_file(fpath):\n",
    "    df = pandas.read_csv(fpath, parse_dates=[0])\n",
    "    df.fillna('', inplace=True)\n",
    "    df.title = df.title.apply(lambda x: html.unescape(x))\n",
    "    df.text = df.text.apply(lambda x: x.replace('\\\\n', '\\n'))\n",
    "    return df\n",
    "\n",
    "\n",
    "def merge_files(ym):\n",
    "    files = []\n",
